{
  "name": "jp9.org",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "server": "nodemon run start",
    "server:install": "npm install",
    "client": "npm run start --prefix client",
    "client:install": "npm install --prefix client",
    "client:build": "npm run build --prefix client",
    "setup": "concurrently \"npm run client:install\" \"npm run server:install\"",
    "dev": "cross-env NODE_ENV=development concurrently \"npm run server\" \"npm run client\""
  },
  "dependencies": {
    "cookie-parser": "~1.4.4",
    "debug": "^4.3.4",
    "decompress": "^4.2.1",
    "express": "^4.16.1",
    "http-errors": "^2.0.0",
    "jsonwebtoken": "^8.5.1",
    "liquidjs": "^10.3.3",
    "morgan": "^1.9.1",
    "multer": "^1.4.5-lts.1",
    "sqlite3": "^5.1.4",
    "telegraf": "^4.11.2"
  },
  "devDependencies": {
    "concurrently": "^7.6.0",
    "cross-env": "^7.0.3",
    "nodemon": "^2.0.20"
  }
}
